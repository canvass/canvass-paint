{
    "name": "canvass/canvass-paint",
    "description": "A form rendering library",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "puritandesigns@gmail.com"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "canvass/canvass-core": "1.00.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "CanvassPaint\\": "src/CanvassPaint/"
        }
    },
    "autoload-dev": {
        "files": [
            "../canvass-core/src/helpers.php"
        ],
        "psr-4": {
            "Implement\\": "tests/Implement/",
            "Tests\\": "tests/Tests/"
        }
    },
    "config": {
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
